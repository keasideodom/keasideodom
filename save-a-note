

let notes = [];

function saveNote(content, id) {
    notes.push({ content, id });
}


saveNote("Buy groceries", 1);
saveNote("Call mom", 2);

console.log(notes);

function logOutNotesFormatted(notes) {
    notes.forEach(note => {
        console.log(`The note with id: ${note.id}, has the following note text: "${note.text}".`);
    });
}